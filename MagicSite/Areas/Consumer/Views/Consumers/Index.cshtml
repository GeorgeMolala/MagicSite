        wrapper.className = 'brand-card col-2';
        wrapper.style.display = 'none'; // Hide all initially

        //Creating content link
        const brandItemLink = document.createElement('a');
        brandItemLink.className = 'slide-link';
        brandItemLink.href = "#";

        //creating content wrapper
        const brandItemWrapper = document.createElement('div');
        brandItemWrapper.className = 'slide-content';

        //creating actual content
        const brandItem = document.createElement('span');
        brandItem.textContent = item.name;

        brandItemWrapper.appendChild(brandItem);
        brandItemLink.appendChild(brandItemWrapper);
        wrapper.appendChild(brandItemLink);
        container.appendChild(wrapper);


        return wrapper;

    })
   

