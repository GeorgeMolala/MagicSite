                    <div class="Catchy-Phrase">
                        <div class="Catchy-Phrase-Support2">
                            <span>Unleashing</span>
                        </div>
                        <div class="Catchy-Phrase-Support1">
                            <span>
                                The Power of movement with Confidence.
                            </span>
                        </div>

                        <div class="Catchy-Shop-Button">
                            <a class="button-styling" href="#">Shop</a>
                        </div>
                    </div>




                </div>
            </div>

        </div>

    </div>
</section>

<section class="Shopping-Highlights" id="Shopping-Highlights">

    <div class="Shop-Section-Head">

        <div class="Gategory-Section row">
            <span class="col-5">
                Shop Categories
            </span>
        </div>

        <div class="Product-Highlights row">

            <div class="card col-4">
                <img src="/CardHighlights/FemaleModel.jpg" />

                <div class="card-catergory">
                    <span>
                        Shop Women
                    </span>
                </div>

                <div class="Shopping-Highlights-button">
                    <button>
                        Shop
                    </button>
                </div>
            </div>

            <div class="card col-4">
                <img src="/CardHighlights/MaleModel.jpg" />

                <div class="card-catergory">
                    <span>
                        Shop Men
                    </span>
                </div>

                <div class="Shopping-Highlights-button">
                    <button>
                        Shop
                    </button>
                </div>
            </div>

            <div class="card col-4">
                <img src="/CardHighlights/KidModel.jpg" />

                <div class="card-catergory">
                    <span>
                        Shop Kids
                    </span>
                </div>

                <div class="Shopping-Highlights-button">
                    <button>
                        Shop
                    </button>
                </div>
            </div>
        </div>

    </div>

</section>

<section id="Hot-Products" class="Hot-Products-Section">
    <div class="Hot-Products-Section-Head">

        <div class="Hot-Products-Section-Title">
            <span class="Hot-Section-Catchy-Phrase-Title">Trending Icons</span>
        </div>

        <div class="Hot-Products-Section-Catalogue row">

            @{
                foreach (var prod in Model.products)
                {
                    <div class="card col-2">
                        <img class="Product-Image" src="@prod.CoverURL" />

                        <div class="Product-Highlights-Clickable">
                            <a href="#" class="Product-Highlights-Clickable-Link">

                                <div class="Product-Highlights-Details">
                                    <div class="Product-Name row">
                                        <span class="col-11">@prod.ProdName</span>
                                    </div>
                                    <div class="Product-Price row">
                                        <span class="col-11">R @prod.ProdPrice</span>

                                    </div>
                                </div>



                            </a>
                        </div>



                    </div>

                }



            }

            <div class="Load-More-Products">
                <br />
                <br />


                <a href="#"> View More..... </a>
            </div>


        </div>

    </div>

</section>

