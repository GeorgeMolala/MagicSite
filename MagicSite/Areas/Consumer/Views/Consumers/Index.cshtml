</script>

<script type="module">
    fetch('/Data/IntroImages.json')
        .then(res => res.json())
        .then(data => {

            const container = document.getElementById("image-sizing");

            const images = data.map(item => {
                // Create a wrapper div
                const wrapper = document.createElement('div');
                wrapper.className = 'sneaker-wrapper col-12';
                wrapper.style.display = 'none'; // Hide all initially

                // Create the image
                const img = document.createElement('img');
                img.src = item.image;
                img.alt = "Sneaker";
                img.className = "sneaker-img";

                // Append image to wrapper, then wrapper to container
                wrapper.appendChild(img);
                container.appendChild(wrapper);

                return wrapper; // Store the wrapper, not the image

            });

            // Start with the first image visible
            let currentIndex = 0;
            if (images.length > 0) {
                images[currentIndex].style.display = 'block';
            }

            // Set up the slideshow timer
            setInterval(() => {
                // Hide current image
                images[currentIndex].style.display = 'none';

                // Move to next image (loop back to 0 if at end)
                currentIndex = (currentIndex + 1) % images.length;

                // Show next image
                images[currentIndex].style.display = 'block';
            }, 3000); // 2000ms = 2 seconds

        });
</script>
